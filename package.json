{
  "name": "@litt1e-p/progress-bar",
  "version": "0.0.1",
  "description": "a progress component with animation effect for vue",
  "config": {
    "libName": "progressBar"
  },
  "keywords": [
    "progress",
    "progress-bar",
    "progress bar",
    "loading",
    "vue",
    "animation",
    "count up effect"
  ],
  "author": "litt1e-p",
  "license": "MIT",
  "main": "dist/progressBar.umd.js",
  "module": "dist/progressBar.esm.js",
  "unpkg": "dist/progressBar.min.js",
  "scripts": {
    "build": "rm -rf dist/ && npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/litt1e-p/progress-bar.git"
  },
  "bugs": {
    "url": "https://github.com/litt1e-p/progress-bar/issues"
  },
  "homepage": "https://github.com/litt1e-p/progress-bar#readme",
  "dependencies": {
    "vue": "^2.6.10",
    "countup.js": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "clean-css": "^4.2.1",
    "cross-env": "^5.2.0",
    "rollup": "^1.19.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue-template-compiler": "^2.6.10"
  }
}
